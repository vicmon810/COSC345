# Doxyfile Example

# Project information
PROJECT_NAME           = "My Documentation"
PROJECT_NUMBER         = 1.0
PROJECT_BRIEF          = "A brief description of my project"
PROJECT_LOGO           = logo.png
OUTPUT_DIRECTORY       = ./docs

# Input files
INPUT                  = ./  # This includes all files in the current directory
FILE_PATTERNS          = *.c *.cpp *.h *.md

# Exclude directories
EXCLUDE_PATTERNS       = */build/*
EXCLUDE                = */tmp/*

# Main page
MAINPAGE               = mainpage

# Other settings (customize as needed)
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
GENERATE_XML           = NO
